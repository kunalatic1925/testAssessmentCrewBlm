{
  "name": "ui-automation-framework-puppeteer",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "pretest": "rimraf dist && tsc",
    "test": "npm run pretest && cucumber-js --config cucumber.mjs --import dist/hooks.js --import 'dist/steps/**/*.js'",
    "posttest": "node scripts/generate-report.mjs && node -e \"require('child_process').exec(process.platform==='win32'?'start '+require('path').resolve('reports/history/index.html'):process.platform==='darwin'?'open '+require('path').resolve('reports/history/index.html'):'xdg-open '+require('path').resolve('reports/history/index.html'))\"",
    "report:enhanced": "node scripts/generate-report.mjs",
    "report:open": "node -e \"require('child_process').exec(process.platform==='win32'?'start '+require('path').resolve('reports/history/index.html'):process.platform==='darwin'?'open '+require('path').resolve('reports/history/index.html'):'xdg-open '+require('path').resolve('reports/history/index.html'))\""
  },
  "dependencies": {
    "@cucumber/cucumber": "^12.2.0",
    "dotenv": "^16.4.5",
    "puppeteer": "^24.27.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "fs-extra": "^11.2.0",
    "multiple-cucumber-html-reporter": "^3.7.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3"
  }
}